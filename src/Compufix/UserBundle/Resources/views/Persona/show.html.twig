{% extends 'CompufixAdminBundle::layout.html.twig' %}
{% block title %}Mensajes{% endblock %}
{% block breadcrumb %}
<li> <a href="{{ path('adminbundle_main_index')}}">Administrador</a> <span class="divider">/</span></li>
<li> <a href="#">{{ 'message.bandeja_salida' |trans({}, 'AdminBundle') }}</a> <span class="divider">/</span></li>
{% endblock %}

{% block javascript_custom %}
<script src="{{ asset('js/compufixadmin/message/index.js') }}" type="text/javascript"></script>
{% endblock %}


{% block content -%}

    {% for flashMessage in app.session.flashbag.get('msg-success') %}
<br>
<div class="alert alert-success">
    <button type="button" class="close" data-dismiss="alert">×</button>
     {{ flashMessage }}.
</div>
<br>						
{% endfor %}

{% for flashMessage in app.session.flashbag.get('msg-error') %}
<br>
<div class="alert alert-error">
    <button type="button" class="close" data-dismiss="alert">×</button>
   {{ flashMessage }}  
</div>
<br>						
{% endfor %}


<h1>Mis datos Personales</h1>

<br>

<table  class="table table-striped table-bordered bootstrap-datatable">
    <tbody>
                <tr>
                    <th>Cedula</th>
                    <td>{{ entity.cedula }}</td>
                </tr>
              
                <tr>
                    <th>Apellidopaterno</th>
                    <td>{{ entity.apellidopaterno }}</td>
                </tr>
                <tr>
                    <th>Apellidomaterno</th>
                    <td>{{ entity.apellidomaterno }}</td>
                </tr>
                <tr>
                    <th>Firstname</th>
                    <td>{{ entity.firstname }}</td>
                </tr>
                <tr>
                    <th>Secondname</th>
                    <td>{{ entity.secondname }}</td>
                </tr>
                <tr>
                    <th>Direccion</th>
                    <td>{{ entity.direccion }}</td>
                </tr>
                <tr>
                    <th>Telefonocasa</th>
                    <td>{{ entity.telefonocasa }}</td>
                </tr>
                <tr>
                    <th>Telefonocelular</th>
                    <td>{{ entity.telefonocelular }}</td>
                </tr>
                <tr>
                    <th>Lugarnacimiento</th>
                    <td>{{ entity.lugarnacimiento }}</td>
                </tr>
                <tr>
                    <th>Sexo</th>
                    <td>{{ entity.sexo }}</td>
                </tr>
                <tr>
                    <th>Estadocivil</th>
                    <td>{{ entity.estadocivil }}</td>
                </tr>
                <tr>
                    <th>Tiposangre</th>
                    <td>{{ entity.tiposangre }}</td>
                </tr>
               
                <tr>
                    <th>Profesion</th>
                    <td>{{ entity.profesion }}</td>
                </tr>
                <tr>
                    <th>Nivelestudio</th>
                    <td>{{ entity.nivelestudio }}</td>
                </tr>

            </tbody>
        </table>

        <ul class="form-actions">

            <li>
                <a href="{{ path('persona_edit', { 'id': entity.id }) }}">
                    Edit
                </a>
            </li>

        </ul>
{% endblock %}
